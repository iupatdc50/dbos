<?xml version="1.0"?><Report><ReportQuery><Format><ReportTitle>Members not Employed by Payor</ReportTitle><ReportDescription>Lists members who were allocated payment by a contractor who is not listed as their current employer </ReportDescription><PageSize>.DEFAULT</PageSize><PageOrientation>.DEFAULT</PageOrientation><TopMargin></TopMargin><BottomMargin></BottomMargin><RightMargin></RightMargin><LeftMargin></LeftMargin><pdfFont></pdfFont><pdfFontSize></pdfFontSize><PreExecuteCode>NONE</PreExecuteCode><formBetweenRows>solidline</formBetweenRows><gridDisplay>.DEFAULT</gridDisplay><gridSortable>.DEFAULT</gridSortable><gridSearchable>.DEFAULT</gridSearchable><gridPageable>.DEFAULT</gridPageable><gridPageSize>.DEFAULT</gridPageSize></Format><Datasource><SourceType>database</SourceType><SourceConnection><DatabaseType>mysql</DatabaseType><DatabaseName></DatabaseName><HostName></HostName><ServiceName></ServiceName><UserName></UserName><Password></Password></SourceConnection></Datasource><EntryForm><Query><TableSql></TableSql><WhereSql></WhereSql><GroupSql></GroupSql><RowSelection>all</RowSelection><SQL><QuerySql></QuerySql><SQLRaw>SELECT
    MS.lob_cd,
    AM.member_id,
    CONCAT(Me.last_nm, ', ', Me.first_nm, COALESCE(CONCAT(' ', Me.middle_inits), ''), COALESCE(CONCAT(', ', Me.suffix), '')) AS full_nm,
    MAX(Re.received_dt) AS last_received_dt,
    MAX(Re.`id`) AS last_receipt,
    Co.contractor AS payor,
    COALESCE (CoC.contractor, 'Unemployed') AS listed
  FROM Receipts AS Re 
    JOIN AllocatedMembers AS AM ON AM.receipt_id = Re.`id`
      JOIN Members AS Me ON Me.member_id = AM.member_id
        JOIN MemberStatuses AS MS ON MS.member_id = Me.member_id 
                                   AND MS.end_dt IS NULL
        LEFT OUTER JOIN CurrentEmployees AS CE ON CE.member_id = Me.member_id
          LEFT OUTER JOIN Contractors AS CoC ON CoC.license_nbr = CE.employer
    JOIN ResponsibleEmployers AS R ON R.receipt_id = Re.`id`
      JOIN Contractors AS Co ON Co.license_nbr = R.license_nbr
  WHERE 1 = 1 
    [ AND MS.lob_cd = {lob_cd} ]
    [ AND Re.received_dt &gt;= {last_received_dt} ]
    AND (R.license_nbr &lt;&gt; CE.employer OR CE.employer IS NULL) 
  GROUP BY lob_cd, full_nm, payor
;
</SQLRaw></SQL><QueryColumns><QueryColumn><Name>lob_cd</Name><TableName>MS</TableName><ColumnName>lob_cd</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Trade</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>full_nm</Name><TableName></TableName><ColumnName>CONCAT(Me.last_nm, ', ', Me.first_nm, COALESCE(CONCAT(' ', Me.middle_inits), ''), COALESCE(CONCAT(', ', Me.suffix), '')) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>show</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title>Member</column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryColumn><QueryColumn><Name>last_received_dt</Name><TableName></TableName><ColumnName>MAX(Re.received_dt) AS</ColumnName><ColumnType>char</ColumnType><ColumnLength>30</ColumnLength><Format><column_display>hide</column_display><content_type>plain</content_type><ColumnStartPDF></ColumnStartPDF><justify>left</justify><ColumnWidthPDF></ColumnWidthPDF><ColumnWidthHTML></ColumnWidthHTML><column_title></column_title><tooltip></tooltip><group_header_label>0</group_header_label><group_header_label_xpos></group_header_label_xpos><group_header_data_xpos></group_header_data_xpos><group_trailer_label>0</group_trailer_label></Format></QueryCol